#include <bits/stdc++.h>
using namespace std;

int main() {
	int n, x; cin >> n >> x;
	vector<int> v(n);
	for(int i = 0; i < n; i++){
		int value;
		cin >> value;
		v[i] = value;
	}
	int left = 0;
	int right = 0;
	int counter = 0;
	int sum = 0;
	while(left < n && right < n){
		if(v[left] == x){
			counter++;
		}
		else if()
		left++;
	}
	cout << counter;
}
